<script type="text/javascript">
$(function(){

function toggleNotes(rowEl, text) {
  let notesEl = rowEl.find('div');
  notesEl.html(text);
  rowEl.toggle(200); // slideToggle doesn't work on 'tr' elements
  notesEl.slideToggle(200);
};
$('.expandable-notes').each(function(){
  toggleNotes($(this));
});
$('.notes-expander').click(function() {
  let rowEl = $(this).closest('tr').nextAll('.expandable-notes').first();
  toggleNotes(rowEl, $(this).attr('data-notes'));
});

});
</script>
